DIM MTIMES[0]
FOR J=0  TO 10
 C=MAINCNT
 FOR I=0 TO 100000
 NEXT I
 M=MAINCNT
 PUSH MTIMES, M-C
NEXT J
'Pull the average of the loop
FOR I=0 TO LEN(MTIMES)-1
 INC SUM, MTIMES[I]
NEXT
AVGTIME=ROUND(SUM/LEN(MTIMES))

C=MAINCNT
FOR I=0 TO 100000
 'Code goes here
NEXT I
M=MAINCNT
PRINT "0."+FORMAT$("%05D",M-C-AVGTIME)

